{
  "kind": "collectionType",
  "collectionName": "students",
  "info": {
    "name": "Student",
    "description": ""
  },
  "options": {
    "increments": true,
    "timestamps": true,
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "password": {
      "type": "password",
      "required": true,
      "minLength": 6,
      "maxLength": 10
    },
    "fullname": {
      "type": "string",
      "required": true
    },
    "email": {
      "type": "email",
      "required": true
    },
    "university": {
      "type": "string"
    },
    "graduation": {
      "type": "datetime"
    },
    "isStudent": {
      "type": "boolean",
      "default": true,
      "required": true
    },
    "isDegree": {
      "type": "boolean"
    },
    "slug": {
      "type": "uid",
      "required": true
    },
    "role": {
      "plugin": "users-permissions",
      "model": "role"
    }
  }
}
